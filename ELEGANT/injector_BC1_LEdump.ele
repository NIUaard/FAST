!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!
! fit quadrupole in pre-BC1 area (same with or without BC1 turned)
!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!
&run_setup    
        lattice="All_Lines_edited/LINES.lte"
! BC1 is off
	use_beamline="INJ_BC1_LEX_LED"
	default_order = 1
        parameters = injector_preBC1.param,
	p_central= "{sddsanalyzebeam daniel_fort1.sdds -pipe=out | sdds2stream -pipe -column=pAverage}"
&end

&run_control
	n_steps = 1,
!    n_indices = 1,
&end


&twiss_output
        matched=0
  	concat_order = 1,        ! for speed
        beta_x  = "{sddsanalyzebeam daniel_fort1.sdds -pipe=out | sdds2stream -pipe -column=betax}",
        beta_y  = "{sddsanalyzebeam daniel_fort1.sdds -pipe=out | sdds2stream -pipe -column=betay}",
        alpha_x = "{sddsanalyzebeam daniel_fort1.sdds -pipe=out | sdds2stream -pipe -column=alphax}",
        alpha_y = "{sddsanalyzebeam daniel_fort1.sdds -pipe=out | sdds2stream -pipe -column=alphay}",
        filename="%s.twi"
&end
!
! ---------------------------------
!
&alter_elements
    name = Q106
    item = K1
    value = 1e-50
&end
&alter_elements
    name = Q107
    item = K1
    value = 1e-50
&end
&alter_elements
    name = Q111
    item = K1
    value = 1e-50
&end
!
! ---------------------------------
! ------- make all quad centered ideal case for now ----
&alter_elements
    name = Q*
    item = DX
    value = 0.0
&end
&alter_elements
    name = Q*
    item = DY
    value = 0.0
&end
&alter_elements
    name = D1*
    item = DX
    value = 0.0
&end
&alter_elements
    name = D1*
    item = DY
    value = 0.0
&end
!!
!
!!!!!! reset intial value of quadrupoles....
!
!!
&alter_elements
    name = Q108
    item = K1
    value =5
&end
&alter_elements
    name = Q109
    item = K1
    value =-5
&end
&alter_elements
    name = Q110
    item = K1
    value = 5
&end
&alter_elements
    name = Q112
    item = K1
    value =-5
&end
&alter_elements
    name = Q113
    item = K1
    value =-0
&end
&alter_elements
    name = Q118
    item = K1
    value = 0
&end
&alter_elements
    name = Q119
    item = K1
    value =-0
&end
&alter_elements
    name = Q120
    item = K1
    value = 2.4
&end
!!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!!

&optimization_setup
        mode = minimize, method = simplex,
        target = 1e-12, tolerance = 1e-14,
        n_passes = 3, n_evaluations = 1500,
        n_restarts = 2,
&end
!!!!! Note
!!!!!
!!!  25 cm target betax works week

&optimization_term
        term = "FITPT_BC1_THIRD#1.betay 10. 1. sene " 
&end       

!&optimization_term
!        term = "FITPT_BC1_THIRD#1.alphay 0 1e-3 sene" 
!&end       

&optimization_term
        term = "FITPT_BC1_THIRD#1.betax 10 1. sene " 
&end

!&optimization_term
!        term = "FITPT_BC1_THIRD#1.alphax 0 1e-3 sene"  
!&end       


&optimization_term
        term = "max.betax 50 1 segt"
&end
&optimization_term
        term = "max.betay 50 1 segt"
&end


!
!!!!!!!!! These are the first two and last three quadrupoles in the first compund section of Elegant, X106_D114
!
&optimization_variable
        name = Q108, item=K1, lower_limit=-30, upper_limit=30, step_size = 0.0001
&end
&optimization_variable
        name = Q109, item=K1, lower_limit=-30, upper_limit=30, step_size = 0.0001
&end
&optimization_variable
        name = Q110, item=K1, lower_limit=-30, upper_limit=30, step_size = 0.0001
&end
&optimization_variable
        name = Q112, item=K1, lower_limit=-30, upper_limit=30, step_size = 0.0001
&end
&optimization_variable
        name = Q113, item=K1, lower_limit=-30, upper_limit=30, step_size = 0.0001
&end
!
!!!!!!!!!
!

&bunched_beam &end

&optimize summarize_setup=1 &end

!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
