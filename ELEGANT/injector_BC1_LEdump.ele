        alpha_y = "{sddsanalyzebeam daniel_fort1.sdds -pipe=out | sdds2stream -pipe -column=alphay}",
        filename="%s.twi"
&end
!
!!
!
!!!!!! reset intial value of quadrupoles....
!
!!
